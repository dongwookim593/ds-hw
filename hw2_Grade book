[['physicals', 98], ['calculus', 97], ['poetry', 85], ['history', 88], ['computer science, 100'], ['visual arts', 98]]
[['physicals', 98], ['calculus', 97], ['history', 88], ['computer science, 100'], ['visual arts', 98], ['poetry', 'pass']]
[['politics', 80], ['latin', 96], ['dance', 97], ['architecture', 65], ['physicals', 98], ['calculus', 97], ['history', 88], ['computer science, 100'], ['visual arts', 98], ['poetry', 'pass']]
